{
    "name": "@airtable/blocks",
    "version": "0.0.35",
    "description": "Airtable Blocks SDK",
    "main": "dist/cjs/index.js",
    "types": "index.d.ts",
    "files": [
        "dist",
        "types",
        "ui.js",
        "models.js",
        "types.js",
        "unstable_private_utils.js",
        "index.d.ts",
        "ui.d.ts",
        "models.d.ts",
        "types.d.ts",
        "unstable_private_utils.d.ts"
    ],
    "scripts": {
        "airbuild": "yarn run build && yarn run docs && yarn run test",
        "pretest": "yarn run lint && yarn run types",
        "version": "changelog-publish --github-repo-url='https://github.com/airtable/blocks' --git-tag-prefix='@airtable/blocks@' && yarn run docs && git add CHANGELOG.md docs/api",
        "release": "npm_config_registry=https://registry.npmjs.org/ release-it",
        "types": "tsc",
        "lint": "eslint --report-unused-disable-directives --ext .js,.ts,.tsx src test",
        "lint:quiet": "yarn run lint --quiet",
        "jest": "jest",
        "jest:watch": "jest --watch",
        "test": "yarn run jest",
        "build:clean": "rm -rf dist",
        "build:babel": "babel src --out-dir dist/cjs --extensions=.js,.ts,.tsx --ignore='**/*.d.ts'",
        "watch:babel": "yarn run build:babel --watch",
        "build:types": "tsc --outDir dist/types --declaration --declarationMap --noEmit false --allowJs false --checkJs false --emitDeclarationOnly --stripInternal",
        "watch:types": "yarn run build:types --watch",
        "docs:clean": "rm -rf ./docs/api ./doc-generator/lib",
        "docs:generator": "tsc -b ./doc-generator/tsconfig.json",
        "docs:typedoc": "typedoc --plugin \"typedoc-plugin-markdown,typedoc-plugin-sourcefile-url,typedoc-plugin-no-inherit,$(pwd)/doc-generator/typedoc-plugin.js\" .",
        "docs:format": "node ./doc-generator/lib/scripts/format_unsupported_tags.js && prettier --write 'docs/**/*.md'",
        "docs": "yarn run docs:clean && yarn run docs:generator && yarn run docs:typedoc && yarn run docs:format",
        "build": "yarn run build:clean && concurrently yarn:build:babel yarn:build:types",
        "watch": "yarn run build:clean && concurrently yarn:watch:babel yarn:watch:types",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@airtable-blocks-internal/changelog-publish": "^1.0.2",
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.6.0",
        "@storybook/addon-actions": "^5.1.10",
        "@storybook/addon-links": "^5.1.10",
        "@storybook/addons": "^5.1.10",
        "@storybook/react": "^5.1.10",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/fs-extra": "^8.0.1",
        "@types/glob": "^7.1.1",
        "@types/jest": "^24.0.18",
        "@types/storybook__react": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^2.3.2",
        "@typescript-eslint/parser": "^2.3.2",
        "babel-eslint": "^11.0.0-beta.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-define": "^1.3.1",
        "concurrently": "^4.1.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "eslint": "5.16.0",
        "eslint-plugin-airtable": "github:hyperbase/eslint-plugin-airtable#01fef1f",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsdoc": "^15.12.0",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "glob": "^7.1.5",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "typedoc": "^0.15.0",
        "typedoc-plugin-markdown": "^2.2.7",
        "typedoc-plugin-no-inherit": "^1.1.10",
        "typedoc-plugin-sourcefile-url": "^1.0.4",
        "typescript": "^3.6.3"
    },
    "dependencies": {
        "@airtable/eslint-plugin-blocks": "^1.0.2",
        "@babel/runtime": "^7.4.4",
        "@styled-system/core": "^5.1.1",
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/lodash.omit": "^4.5.6",
        "@types/prop-types": "^15.7.3",
        "@types/react": "^16.9.5",
        "@types/react-dom": "^16.9.1",
        "@types/react-window": "^1.8.1",
        "@types/styled-system": "^5.1.1",
        "core-js": "^3.0.1",
        "emotion": "^10.0.14",
        "fast-deep-equal": "^2.0.1",
        "hoist-non-react-statics": "^3.3.0",
        "lodash.omit": "^4.5.0",
        "prop-types": "15.7.2",
        "react-window": "1.8.5",
        "use-subscription": "^1.0.0"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/test/setup_enzyme.ts"
        ]
    },
    "release-it": {
        "git": {
            "tagName": "@airtable/blocks@${version}",
            "commitMessage": "Release @airtable/blocks@${version}"
        },
        "hooks": {
            "before:init": "../../bin/check-repo-for-release && yarn test",
            "after:release": "git push --tags git@github.com:Airtable/blocks.git master"
        },
        "npm": {
            "access": "restricted"
        }
    }
}
