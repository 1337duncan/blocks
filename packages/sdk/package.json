{
    "name": "@airtable/blocks",
    "version": "0.0.29",
    "description": "Airtable Blocks SDK",
    "main": "dist/cjs/index.js",
    "files": [
        "dist",
        "ui.js",
        "models.js"
    ],
    "scripts": {
        "prepare": "yarn run build",
        "pretest": "yarn run lint && yarn run flow",
        "version": "changelog-publish --github-repo-url='https://github.com/airtable/blocks' --git-tag-prefix='@airtable/blocks@' && yarn run build:docs && git add CHANGELOG.md docs/api.md",
        "release": "npm_config_registry=https://registry.npmjs.org/ release-it",
        "flow": "flow",
        "lint": "eslint *.js src test",
        "lint:quiet": "yarn run lint --quiet",
        "jest": "jest",
        "jest:watch": "jest --watch",
        "test": "yarn run jest",
        "build:clean": "rm -rf dist",
        "build:babel": "babel src --out-dir dist/cjs",
        "watch:babel": "yarn run build:babel --watch",
        "build:flow": "flow-copy-source src dist/cjs",
        "watch:flow": "yarn run build:flow --watch",
        "build:docs:noformat": "documentation build src/index.js --format md --markdown-toc --markdown-toc-max-depth 3 --config documentation_config.yml --sort-order alpha --infer-private '^_' --output docs/api.md && yarn run validate-markdown-links docs/api.md",
        "build:docs": "yarn run build:docs:noformat && prettier --write docs/api.md",
        "watch:docs": "yarn run build:docs:noformat --watch",
        "validate-markdown-links": "remark --no-stdout",
        "build": "yarn run build:clean && yarn run build:babel && yarn run build:flow && yarn run build:docs",
        "watch": "yarn run build:clean && concurrently yarn:watch:babel yarn:watch:flow",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@airtable-blocks-internal/changelog-publish": "^1.0.2",
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-syntax-flow": "^7.2.0",
        "@babel/plugin-transform-flow-strip-types": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.1.10",
        "@storybook/addon-links": "^5.1.10",
        "@storybook/addons": "^5.1.10",
        "@storybook/react": "^5.1.10",
        "babel-eslint": "^11.0.0-beta.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-define": "^1.3.1",
        "concurrently": "^4.1.0",
        "documentation": "^11.0.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "eslint": "5.16.0",
        "eslint-plugin-airtable": "github:hyperbase/eslint-plugin-airtable#01fef1f",
        "eslint-plugin-flowtype": "3.9.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsdoc": "^7.2.3",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "flow-bin": "0.98.1",
        "flow-copy-source": "^2.0.6",
        "flow-typed": "^2.5.2",
        "fs-extra": "^8.0.1",
        "glob": "^7.1.4",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "remark-cli": "^7.0.0",
        "remark-lint-no-dead-urls": "^0.5.0",
        "remark-validate-links": "^9.0.1"
    },
    "dependencies": {
        "@airtable/eslint-plugin-blocks": "^1.0.2",
        "@babel/runtime": "^7.4.4",
        "@styled-system/core": "^5.0.18",
        "core-js": "^3.0.1",
        "emotion": "^10.0.14",
        "fast-deep-equal": "^2.0.1",
        "lodash.omit": "^4.5.0",
        "prop-types": "15.7.2",
        "react-window": "1.8.5"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/test/setup_enzyme.js"
        ]
    },
    "release-it": {
        "git": {
            "tagName": "@airtable/blocks@${version}",
            "commitMessage": "Release @airtable/blocks@${version}"
        },
        "hooks": {
            "before:init": "../../bin/check-repo-for-release && yarn test",
            "after:release": "git push --tags git@github.com:Airtable/blocks.git master"
        },
        "npm": {
            "access": "restricted"
        }
    }
}
